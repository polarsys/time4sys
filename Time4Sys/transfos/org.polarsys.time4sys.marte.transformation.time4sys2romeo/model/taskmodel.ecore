<?xml version="1.0" encoding="UTF-8"?>
<ecore:EPackage xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:ecore="http://www.eclipse.org/emf/2002/Ecore" name="taskmodel" nsURI="http://org.polarsys.time4sys.marte.transformation.petrinet.taskmodel"
    nsPrefix="taskmodel">
  <eClassifiers xsi:type="ecore:EClass" name="System">
    <eOperations name="convertToPetriNet" eType="ecore:EClass petrinet.ecore#//PetriNet"/>
    <eOperations name="clone" eType="#//System"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="observers" upperBound="-1"
        eType="#//Observer" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedTasks" upperBound="-1"
        eType="#//Task" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedMutualExclusionResources"
        upperBound="-1" eType="#//MutualExclusionResource" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Task">
    <eOperations name="clone" eType="#//Task"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="stimulus" lowerBound="1"
        eType="#//Stimulus" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="allFragments" upperBound="-1"
        eType="#//AbstractFragment" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="fragments" upperBound="-1"
        eType="#//Fragment" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="sharingResourceFragments"
        upperBound="-1" eType="#//SharingResourceFragment" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Observer" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedTransitions" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Transition" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Connection" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedPlaces" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Place" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="checkPlaces" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Place"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Stimulus">
    <eOperations name="clone" eType="#//Stimulus"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="offset" eType="#//Offset"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="firstRelease" eType="#//First_Release"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="period" eType="#//Period"
        containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="jitter" eType="#//Jitter"
        containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="First_Release" eSuperTypes="#//SimpleComponent">
    <eOperations name="clone" eType="#//First_Release"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Period" eSuperTypes="#//SimpleComponent">
    <eOperations name="clone" eType="#//Period"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Jitter" eSuperTypes="#//SimpleComponent">
    <eOperations name="clone" eType="#//Jitter"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Fragment" eSuperTypes="#//SimpleComponent #//AbstractFragment">
    <eOperations name="clone" eType="#//Fragment"/>
    <eStructuralFeatures xsi:type="ecore:EAttribute" name="priority" eType="ecore:EDataType http://www.eclipse.org/emf/2002/Ecore#//EInt"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="Offset" eSuperTypes="#//SimpleComponent">
    <eOperations name="clone" eType="#//Offset"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="BoundObserver" eSuperTypes="#//Observer"/>
  <eClassifiers xsi:type="ecore:EClass" name="ResponseTimeObserver" eSuperTypes="#//Observer"/>
  <eClassifiers xsi:type="ecore:EClass" name="SimpleComponent" abstract="true">
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedTransition" lowerBound="1"
        eType="ecore:EClass petrinet.ecore#//Transition" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Connection" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="ownedPlace" lowerBound="1"
        eType="ecore:EClass petrinet.ecore#//Place" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="SharingResourceFragment" eSuperTypes="#//AbstractFragment">
    <eOperations name="clone" eType="#//SharingResourceFragment"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="firstFragment" lowerBound="1"
        eType="#//Fragment" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="secondFragment" lowerBound="1"
        eType="#//Fragment" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Connection" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="MutualExclusionResource">
    <eOperations name="clone" eType="#//MutualExclusionResource"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="mutualExclusionResourcePlace"
        lowerBound="1" eType="ecore:EClass petrinet.ecore#//Place" containment="true"/>
    <eStructuralFeatures xsi:type="ecore:EReference" name="connections" upperBound="-1"
        eType="ecore:EClass petrinet.ecore#//Connection" containment="true"/>
  </eClassifiers>
  <eClassifiers xsi:type="ecore:EClass" name="AbstractFragment">
    <eStructuralFeatures xsi:type="ecore:EReference" name="task" eType="#//Task"/>
  </eClassifiers>
</ecore:EPackage>
