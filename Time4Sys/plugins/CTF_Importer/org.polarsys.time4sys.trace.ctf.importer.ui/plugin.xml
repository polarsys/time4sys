<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.popupMenus">
      <objectContribution
            id="org.polarsys.time4sys.trace.ctf.importer.ui.contribution1"
            nameFilter="*.ctf"
            objectClass="org.eclipse.core.resources.IFile">
         <menu
               icon="icons/Time4Sys16x16.png"
               id="org.polarsys.time4sys.trace.ctf.importer.ui.menu1"
               label="Import ctf Trace"
               path="additions">
            <separator
                  name="group1">
            </separator>
         </menu>
         <action
               class="org.polarsys.time4sys.trace.ctf.importer.ui.popup.actions.ImportctftoSTAction"
               enablesFor="*"
               id="org.polarsys.time4sys.trace.ctf.importer.ui.newAction2"
               label="ctf Trace Import to Scheduling Trace"
               menubarPath="org.polarsys.time4sys.trace.ctf.importer.ui.menu1/group1">
         </action>
      </objectContribution>
   </extension>

</plugin>
