<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="org.eclipse.jdt.ui.JavaPerspective">
         <view
               id="org.polarsys.time4sys.graphics.partition.PartitionView"
               ratio="0.5"
               relationship="right"
               relative="org.eclipse.ui.views.ProblemView">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.help.contexts">
      <contexts
            file="contexts.xml">
      </contexts>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <category
            id="org.polarsys.time4sys.graphics.partition"
            name="Time4Sys">
      </category>
      <view
            category="org.polarsys.time4sys.graphics.partition"
            class="org.polarsys.time4sys.graphics.partition.ui.PartitionViewPart"
            icon="icons/partition.gif"
            id="org.polarsys.time4sys.graphics.partition.PartitionView"
            name="Partition">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.polarsys.time4sys.graphics.partition.ui.action.StartupAction">
      </startup>
   </extension>

</plugin>
